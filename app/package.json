  "bugs": {
    "url": "https://github.com/brittverschueren/zoeff/issues"
  },
  "homepage": "https://github.com/brittverschueren/zoeff#readme",
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6"
  },
  "devDependencies": {
    "serve": "5.1.5",
    "node-sass": "4.5.3",
    "concurrently": "3.4.0",
    "vex-js": "3.1.1"
  }
}