{
  "name": "Comicshop",
  "version": "1.0.0",
  "description": "https://nodejs.org/en/ v7.10.0",
  "main": "index.js",
  "scripts": {
    "css": "node-sass styles/style.scss -o css --output-style compressed",
    "css:watch": "npm run css && node-sass styles/style.scss -wo css --output-style compressed",
    "serve": "serve ./",
    "start": "concurrently --kill-others \"npm run css:watch\" \"npm run serve\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brittverschueren/zoeff.git"
  },
  "keywords": [],
  "author": "",
